  "version": "0.2.0",
  "configurations": [
    {
      "name": "vtab",
      "type": "debugpy",
      "request": "launch",
      "program": "main.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/binary_adapter",
      "env": {
        "CUDA_VISIBLE_DEVICES": "0",
        "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2"
      },
      "args": ["--dataset", "oxford_iiit_pet", "--method", "adaptformer-bihead", "--dim", "32", "--bit", "1", "--load_config", "--model_path", "./ckpts"],
      "justMyCode": true
    },
  ]
}